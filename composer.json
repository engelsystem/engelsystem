        "vlucas/phpdotenv": "^5.3",
        "bahuma/oauth2-nextcloud": "^1.1"
    },
    "require-dev": {
        "dms/phpunit-arraysubset-asserts": "^0.3.0",
        "filp/whoops": "^2.12",
