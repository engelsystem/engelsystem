    "autoload": {
        "psr-4": {
            "Engelsystem\\": "src/"
        },
        "classmap": ["db/migrations"],
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Engelsystem\\Test\\": "tests/"
        }
