{
  "parser": "@babel/eslint-parser",
  "extends": [ "plugin:editorconfig/all" ],
  "plugins": [ "editorconfig" ],
  "rules": {
    "prefer-arrow-callback": "error",
    "prefer-template": "error",
    "no-var": "error",
    "quotes": [
      "error",
      "single",
      {
        "avoidEscape": true
      }
    ]
  }
}
